<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="DicionarioPattern">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" name="registro" type="tc_registro"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:complexType name="tc_registro">
    <xs:sequence>
      <xs:element name="pattern" type="xs:string"/>
      <xs:element name="mensagem" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>

</xs:schema>
